{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "object-curly-spacing": "off",
    "arrow-parens": "off",
    "indent": ["error", 4],
    "require-jsdoc": "off",
    "comma-dangle": "off",
    "max-len": ["error", 130],
    "no-unused-vars": "off",
    "no-extend-native": "off",
    "no-case-declarations": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
